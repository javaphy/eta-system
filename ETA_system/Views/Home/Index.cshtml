@using ETA_system.Models
<table class="archive" cellpadding="0" cellspacing="0">
    <thead>
        <tr>
            <th>Order Number</th>
            <th>Customer Name</th>
            <th>Machine Model</th>
            <th>Machine Serial Number</th>
            <th>Status</th>
            <th>Province</th>
            <th>Place Order Time</th>
            <th>Print Order Time</th>
            <th>Received In Preflight Time</th>
            <th>Estimated Time of Complete</th>
            <th>Planned Delivery Time</th>
            <th>Actual Preflight Complete Time</th>
            <th>Tech Name</th>
            <th>Comments</th>
        </tr>
    </thead>
    
    <tbody>
        @foreach (WorkOrder row in ViewBag.Rows)
        {
            <tr>
                <td>@row.OrderNumber</td>
                <td>@row.CustomerName</td>
                <td>@row.MachineModel</td>
                <td>@row.MachineSerialNumber</td>
                <td>@row.Status</td>
                <td>@row.Province</td>
                <td class="date">@row.PlaceOrderTime.ToShortDateString()</td>
                <td class="date">@row.PrintOrderTime.ToShortDateString()</td>
                <td class="time">@row.ReceivedInPreflightTime</td>
                <td class="date">@row.EstimatedTimeOfComplete.ToShortDateString()</td>
                <td class="date">@row.PlannedDeliveryTime.ToShortDateString()</td>
                <td class="time">@row.ActualPreflightCompleteTime</td>
                <td>@row.TechName</td>
                <td>@row.Comments</td>
            </tr>
        }
    </tbody>
</table>